{
  "version": 2,
  "projects": {
    "c": {
      "root": "libs/c",
      "sourceRoot": "libs/c/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/c/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/c"
          ],
          "options": {
            "jestConfig": "libs/c/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "d": {
      "root": "libs/d",
      "sourceRoot": "libs/d/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/d/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/d"
          ],
          "options": {
            "jestConfig": "libs/d/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "a": {
      "root": "apps/a",
      "sourceRoot": "apps/a/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/a",
            "index": "apps/a/src/index.html",
            "main": "apps/a/src/main.tsx",
            "polyfills": "apps/a/src/polyfills.ts",
            "tsConfig": "apps/a/tsconfig.app.json",
            "assets": [
              "apps/a/src/favicon.ico",
              "apps/a/src/assets"
            ],
            "styles": [],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/a/src/environments/environment.ts",
                  "with": "apps/a/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "a:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "a:build:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/a/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/a"
          ],
          "options": {
            "jestConfig": "apps/a/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "a-e2e": {
      "root": "apps/a-e2e",
      "sourceRoot": "apps/a-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/a-e2e/cypress.json",
            "tsConfig": "apps/a-e2e/tsconfig.e2e.json",
            "devServerTarget": "a:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "a:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/a-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    },
    "b": {
      "root": "apps/b",
      "sourceRoot": "apps/b/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/b",
            "index": "apps/b/src/index.html",
            "main": "apps/b/src/main.tsx",
            "polyfills": "apps/b/src/polyfills.ts",
            "tsConfig": "apps/b/tsconfig.app.json",
            "assets": [
              "apps/b/src/favicon.ico",
              "apps/b/src/assets"
            ],
            "styles": [],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/b/src/environments/environment.ts",
                  "with": "apps/b/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "b:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "b:build:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/b/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/b"
          ],
          "options": {
            "jestConfig": "apps/b/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "b-e2e": {
      "root": "apps/b-e2e",
      "sourceRoot": "apps/b-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/b-e2e/cypress.json",
            "tsConfig": "apps/b-e2e/tsconfig.e2e.json",
            "devServerTarget": "b:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "b:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/b-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "styled-components",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "styled-components"
      },
      "library": {
        "style": "styled-components",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "a"
}